<h1><%= "Create new events for #{@meet.name}" %></h1>

<table>
  <tr>
    <th></th>
    <th>400 IH (M)</th>
    <th>400 IM (F)</th>
    <th></th>
    <th>Two Hundred Meter Dash</th>
  </tr>
  <% @athletes.each do |athlete| %>
    <tr>
      <td>
        <%= athlete.name %>
      </td>
      <td>
        <%= check_box_tag("#{athlete.name}".to_sym) %>
      </td>
    </tr>
  <% end %>
</table>