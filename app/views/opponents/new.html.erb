<h1>Add Opponents</h1>
<div class="row">
  <div class="span6">
    <%= render "form" %>
  </div>
  <div class="span6">
    <% @opponents_already_created.each do |opponent|  %>
      <ul>
        <li><%= School.find_by_id(opponent.school_id).name %></li>
      </ul>
    <% end %>
  </div>
</div>

<%= link_to "Finished", season_meet_path(@season, @meet), :class => 'btn' %>
