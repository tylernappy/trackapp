<div class="row">
  <div class="span6">
    <% if @season.winter == true %>
      <h3><%= "Winter #{@season.year}" %></h3>
    <% elsif @season.spring == true %>
      <h3><%= "Spring #{@season.year}" %></h3>
    <% end %>
  </div>
</div>

<%= link_to "Back", seasons_path, :class => 'btn' %>
<%= link_to "Edit", edit_season_path(@season), :class => 'btn' %>
<%= link_to "New Meet", new_season_meet_path(@season), :class => 'btn btn-success' %>
<%= link_to "See all meets", season_meets_path(@season), :class=> 'btn' %>