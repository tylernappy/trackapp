<%= form_for ([current_coach, @athlete]), :html => { :mulitpart => true } do |f| %>
  
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :grade %>
  <%= f.text_field :grade %>
  <%= f.label :boy %>
  <%= f.check_box :boy %>
  <%= f.label :girl %>
  <%= f.check_box :girl %>


  <dt><%= f.submit "Submit", :class => 'btn' %></dt>

<% end %>
